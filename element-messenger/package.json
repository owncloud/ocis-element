{
  "name": "element-messenger",
  "version": "0.1",
  "private": true,
  "description": "Element Messenger basic integration",
  "scripts": {
    "lint": "eslint src/**/*.vue src/**/*.js --color --global requirejs --global require",
    "lint-fix": "eslint src/**/*.vue src/**/*.js --color --global requirejs --global require --fix",
    "watch": "webpack --progress --colors --watch --mode development --config webpack.dev.js",
    "build": "cd element-file-picker; yarn build; cd ..; rollup -c",
    "postinstall": "cd element-file-picker; yarn install"
  },
  "author": "Lars Luz",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@vue/compiler-sfc": "^3.0.9",
    "css-loader": "^3.1.0",
    "postcss": "^8.2.8",
    "rollup": "^2.41.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
